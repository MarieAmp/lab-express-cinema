<a href="/movies">Go Back</a>
<div id="one-movie">
    <img src={{movie.image}} alt={{movie.title}}>
    <h1>{{movie.title}}</h1>
    <h2>{{movie.director}}</h2>
    {{#each movie.stars}}
    <li>{{this}}</li>
    {{/each}}
    <p>{{movie.description}}
    </p>
    <div id="showtimes">
        {{#each movie.showtimes}}
        <li>{{this}}</li>
        {{/each}}
    </div>

</div>